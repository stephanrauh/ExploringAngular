    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "protractor": "~5.1.0",
    "ts-node": "3.0.2",
    "tslint": "^5.0.0",
    "typescript": "~2.2.2"
  }
